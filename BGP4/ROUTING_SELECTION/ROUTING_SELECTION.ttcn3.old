					}	
				}
			}// end function	fun_ESTABLISHED_PCO2()
			
			function	fun_ptc_UPDATE_AS_PATH_1() runs on BGP_PTC_Component
			{
				var 	integer	HoldTimerValue := 190;
				var	integer	KeepTimerValue := 60;
								
				fun_ESTABLISHED_PCO2();
				BGP_ConnectTimer.start;
				alt	//01
