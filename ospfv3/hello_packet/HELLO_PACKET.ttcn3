	function P3() runs on MyTestComponentAsync
	{
		var t_DDP3 DDP3var;
		var t_LSAHDR LSAHDRvar;
		var Oct4 rid;
		var Oct4 lid;
		var Oct4 pid;
		var t_LSU3_R0L1P1 lsu3var ;
		var t_LLSA llsavar;
		var t_RLSA0 rlsavar;		
		var t_PLSA plsavar;
		var Oct2 age1 ; var Oct4 seq1 ; var Oct4 id1 ;var Oct2 chk1;var Oct2 len1;
		var Oct2 age2 ; var Oct4 seq2 ; var Oct4 id2 ;var Oct2 chk2;var Oct2 len2;
		var Oct2 age3 ; var Oct4 seq3 ; var Oct4 id3 ;var Oct2 chk3;var Oct2 len3;
		var Oct2 age4 ; var Oct4 seq4 ; var Oct4 id4 ;var Oct2 chk4;var Oct2 len4;
		P2();
		MyPortAsync.send(TS_DDP0('111'B,con_Seq1,mp_TS_RID));
		label P3A;
		alt{
			[] MyPortAsync.receive(IUT_DDP3(con_Seq1))->value DDP3var
			{
				LSAHDRvar :=DDP3var.LSA1;lid :=LSAHDRvar.ID;
				LSAHDRvar :=DDP3var.LSA2;rid :=LSAHDRvar.ID;
				LSAHDRvar :=DDP3var.LSA3;pid :=LSAHDRvar.ID;
				MyPortAsync.send(TS_LSR3(lid,rid,pid));
				label P3B;
				alt{
					//[]Ack_LSU3_ROL1P1()
					//{}
					[]MyPortAsync.receive(IUT_LSU3_R0L1P1)->value lsu3var
					{
						llsavar :=lsu3var.LSA1;LSAHDRvar :=llsavar.Hdr;
						age1 :=LSAHDRvar.Age;len1 :=LSAHDRvar.Len;id1 :=LSAHDRvar.ID;seq1 :=LSAHDRvar.Seq;chk1 :=LSAHDRvar.Chk;
						rlsavar :=lsu3var.LSA2;LSAHDRvar :=rlsavar.Hdr;
						age2 :=LSAHDRvar.Age;len2:=LSAHDRvar.Len;id2 :=LSAHDRvar.ID;seq2:=LSAHDRvar.Seq;chk2 :=LSAHDRvar.Chk;
						plsavar :=lsu3var.LSA3;LSAHDRvar :=plsavar.Hdr;
						age3 :=LSAHDRvar.Age;len3 :=LSAHDRvar.Len;id3 :=LSAHDRvar.ID;seq3 :=LSAHDRvar.Seq;chk3 :=LSAHDRvar.Chk;
						
						
						MyPortAsync.send(TS_Ack3(con_Ltype,con_Rtype,con_Ptype,age1,age2,age3,id1,id2,id3,seq1,seq2,seq3,chk1,chk2,chk3,len1,len2,len3));
						myTimer.start(con_RtxTM);
						label Ack_LSU3_B;
						alt{
							[]myTimer.timeout
							{}
							[]MyPortAsync.receive(IUT_HL1(mp_IUT_RID,con_ZERO4))
							{
								MyPortAsync.send(TS_HL1(mp_IUT_RID,con_ZERO4,con_Pri0));
								goto Ack_LSU3_B;
							}
							[]MyPortAsync.receive(IUT_HL1(mp_TS_RID,mp_IUT_RID))
							{
								MyPortAsync.send(TS_HL1(mp_TS_RID,mp_IUT_RID,con_Pri3));
								goto Ack_LSU3_B;
							}
							[] MyPortAsync.receive(IUT_LSU3_R0L1P1)
							{
								MyPortAsync.send(TS_Ack3(con_Ltype,con_Rtype,con_Ptype,age1,age2,age3,id1,id2,id3,seq1,seq2,seq3,chk1,chk2,chk3,len1,len2,len3));
								goto Ack_LSU3_B;
							}
							[] MyPortAsync.receive(IUT_LSU2_R1P1)
							{
								goto Ack_LSU3_B;
							}
							[]MyPortAsync.receive(IUT_LSU1_R0)
							{
								rlsavar :=IUT_LSU1_R0.LSA1;LSAHDRvar :=rlsavar.Hdr;
								age4 :=LSAHDRvar.Age;len4:=LSAHDRvar.Len;id4 :=LSAHDRvar.ID;seq4:=LSAHDRvar.Seq;chk4 :=LSAHDRvar.Chk;
								MyPortAsync.send(TS_Ack1(con_Rtype,age4,id4,seq4,chk4,len4));
								goto Ack_LSU3_B;
							}							
							[] MyPortAsync.receive
							{
								setverdict(fail);
								stop;
							}
						}
					}
					[] MyPortAsync.receive(IUT_HL1(mp_IUT_RID,con_ZERO4))
					{
						goto P3B;
					}
					[] MyPortAsync.receive
					{
						setverdict(fail);
						stop;
					}						
				}
			}
			[] MyPortAsync.receive(IUT_DDP0_1_Filter_wait('111'B))
			{
				goto P3A;
			}
			[] MyPortAsync.receive(IUT_HL1(mp_IUT_RID,con_ZERO4))
			{
				goto P3A;
			}
			[] MyPortAsync.receive
			{
				setverdict(fail);
				stop;
			}			
		}
	}
	
