	function P1() runs on MyTestComponentAsync
	{
		P0();
		MyPortAsync.send(TS_HLO(mp_IUT_Opt,con_Hello_interval,con_Dead_interval));

		myTimer.start(con_HelloTM) ;
		var integer MyVar := 2;
		label L2; 
		alt {
			[] MyPortAsync.receive(IUT_HL1(mp_IUT_RID,con_ZERO4))
			{
				//setverdict(pass);
			}
			[] myTimer.timeout			
			{
				MyVar :=MyVar-1;
				if(MyVar>0)
				{
					myTimer.start(con_HelloTM) ;
					goto L2;
				}
				setverdict(fail);
				stop;
			}
			[] MyPortAsync.receive
			{
				setverdict(fail);
				stop;
			}				
		}
	}
	
